<%- cards.each do |card| -%>
<div id="card_<%= card.id %>">
<%- form_for(card, :method => :put) do -%>
Title: <%= card.title %><br/>
Description: <%= card.description %><br/>
Points: <%= card.points %><br/>
<%= submit_tag "Move to:" %> <%= select(:card, :iteration_id, Iteration.all.map { |iteration| ["Iteration: #{iteration.start_date.to_s(:iteration_start_date)}", iteration.id]}) %>
<%- end -%>
</div>
<%- end -%>
